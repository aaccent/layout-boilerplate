    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "image-minimizer-webpack-plugin": "^4.0.0",
    "imask": "^7.6.1",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
